    "next-redux-wrapper": "2.0.0-beta.6",
    "react-dom": "16.4.1",
    "normalize.css": "8.0.0",
    "node-sass": "4.9.2 ",
    "@zeit/next-sass": "^0.2.0",
    "redux": "4.0.0",
    "react-redux": "5.0.7",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "redux-promise-middleware": "5.1.1",
    "axios": "0.18.0",
    "cross-env": "5.2.0",
    "webpack-bundle-analyzer": "2.13.1",
    "prismic-javascript": "1.5.0-beta.4",
